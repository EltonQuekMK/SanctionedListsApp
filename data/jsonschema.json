{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "TYPE": {
                "type": "string",
                "enum": [
                    "individual",
                    "entity"
                ]
            },
            "FIRST_NAME": {
                "type": "string"
            },
            "SECOND_NAME": {
                "type": "string"
            },
            "THIRD_NAME": {
                "type": "string"
            },
            "ALIAS": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "ALIAS_NAME": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "ALIAS_NAME"
                    ]
                }
            },
            "ADDRESS": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
            "DATE_OF_BIRTH": {
                "type": "string"
            },
            "PLACE_OF_BIRTH": {
                "type": "object",
                "properties": {
                    "COUNTRY": {
                        "type": "string"
                    },
                    "CITY": {
                        "type": "string"
                    },
                    "STATE_PROVINCE": {
                        "type": "string"
                    }
                }
            },
            "NAME_ORIGINAL_SCRIPT": {
                "type": "string"
            },
            "TITLE": {
                "type": "string"
            },
            "DESIGNATION": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            },
            "NATIONALITY": {
                "type": "string"
            },
            "COMMENTS": {
                "type": "string"
            }
        },
        "required": [
            "TYPE",
            "FIRST_NAME"
        ]
    }
}